//Input "1006.765, 405.486 1006.979, 402.069 1004.523"
//Return coordinates formated like this: ["1006.765,405.486"], ["1006.979,402.069 1004.523"]
function coordArray(coordString) {
    let re = new RegExp(", ", 'g');
    let coords = coordString.replace(re, ",").split(" ");
    let temp = coords.slice();
    let arr = [];

    while (temp.length) {
        arr.push(temp.splice(0, 1));
    }
    return arr;
}
